{
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "babel-plugin-transform-decorators-legacy"
    ]
  },
  "bundleDependencies": false,
  "dependencies": {
    "@types/enzyme": "^3.1.11",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/history": "^4.7.2",
    "@types/jest": "^20.0.8",
    "@types/jquery": "^3.3.29",
    "@types/node": "^8.10.39",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-redux": "^5.0.17",
    "@types/redux-logger": "^3.0.6",
    "@types/redux-promise-middleware": "0.0.8",
    "@types/sinon": "^5.0.7",
    "ansi-colors": "^3.2.3",
    "axios": "0.17.0",
    "babelify": "^7.3.0",
    "body-parser": "~1.18.2",
    "browser-sync": "^2.26.3",
    "browserify": "^16.2.3",
    "browserify-css": "^0.14.0",
    "crypto": "^1.0.1",
    "crypto-js": "^3.1.9-1",
    "date-input-polyfill": "^2.14.0",
    "dotenv": "^6.2.0",
    "envify": "^4.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "express": "^4.16.3",
    "express-react-views": "^0.10.5",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-plumber": "^1.2.1",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.4",
    "history": "^4.6.3",
    "https": "^1.0.0",
    "inversify": "^4.13.0",
    "inversify-express-utils": "^6.1.0",
    "jest": "^22.4.3",
    "jquery": "3.2.1",
    "localenvify": "^1.0.1",
    "moment": "^2.23.0",
    "node-notifier": "^5.3.0",
    "nodemon": "^1.17.5",
    "path": "^0.12.7",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-minimal-pie-chart": "^3.3.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.0.0",
    "redux-promise-middleware": "^4.3.0",
    "redux-thunk": "^2.3.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "rxios": "github:begarland/rxios",
    "rxjs": "^6.2.0",
    "rxjs-compat": "^6.2.1",
    "sinon": "^4.5.0",
    "ts-jest": "^22.4.2",
    "ts-node": "^7.0.0",
    "tsify": "^4.0.1",
    "tslint": "^5.12.1",
    "typescript": "^2.8.1",
    "uglifyify": "^4.0.5",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.0",
    "winston": "^2.4.2"
  },
  "deprecated": false,
  "description": "some sample-app description",
  "homepage": "https://github.com/begarland/react-redux-express-template#readme",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)(spec|test).tsx?(x)"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "name": "t3-app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/begarland/react-redux-express-template.git"
  },
  "scripts": {
    "dev": "npm run watch-server & gulp --silent",
    "lint": "tslint 'src/**/*.ts' --force -t codeFrame",
    "prod": "npm run production-sass & npm run production-web",
    "production-sass": "sass --sourcemap=none web/src/main.scss:web/public/main.css",
    "production-web": "browserify web/src/index.tsx -p tsify -g [ envify --NODE_ENV production ] -g [ uglifyify ] -o web/public/bundle.js -v",
    "start": "npm run prod; node_modules/.bin/ts-node server/index.ts",
    "test": "jest --color --verbose --expand --watchAll",
    "watch-sass": "sass --watch web/src/main.scss:web/public/main.css",
    "watch-server": "NODE_ENV=development node_modules/.bin/nodemon --exec 'yarn run lint && node_modules/.bin/ts-node server/index.ts'",
    "watch-typescript": "watchify web/src/index.tsx -p tsify -g [ localenvify --envfile .env ]--debug -o web/public/bundle.js -v",
    "watch-web": "npm run watch-sass & npm run watch-typescript"
  },
  "version": "0.0.0",
  "private": true,
  "license": "BSD"
}
